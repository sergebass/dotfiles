set number
set cursorline

set hlsearch
set incsearch

set tabstop=4
set expandtab
set shiftwidth=4

set textwidth=120
set scrolloff=1

set startnormalmode
set contentassistmode

set clipboard=unnamed, unnamedplus

let mapleader='\\'

nnoremap <CR> <C-]>

nnoremap <Space><Space> <Esc>:

nnoremap <Space>w <C-W>

nnoremap <Space>y "+y
nnoremap <Space>p "+p
nnoremap <Space>P "+P

nnoremap <Space>fs :w<CR>
