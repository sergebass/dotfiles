set number
set cursorline

set hlsearch
set incsearch

set tabstop=4
set expandtab
set shiftwidth=4

set textwidth=120
set scrolloff=1

set startnormalmode
set contentassistmode

set clipboard=unnamedplus

let mapleader='\\'

nnoremap <CR> <C-]>

nnoremap S :wa<CR>
nnoremap Q :qa<CR>

nnoremap <Leader>v :vim<CR>

nnoremap <Leader>y "+y
nnoremap <Leader>p "+p
nnoremap <Leader>P "+P

nnoremap <Space><Space> <Esc>:
nnoremap <Space><Tab> <C-^>
nnoremap <Space>w <C-W>

nnoremap <Space>qq :qa<CR>

nnoremap <Space>fs :w<CR>
nnoremap <Space>fS :wa<CR>
nnoremap <Space>fed :new<CR>:e ~/.vrapperrc<CR>

nnoremap <Space>bb :files<CR>
nnoremap <Space>bd :bd<CR>
nnoremap <Space>bn :bnext<CR>
nnoremap <Space>bp :bprev<CR>
nnoremap <Space>bR :e!
nnoremap <Space>bY gg"+yG
nnoremap <Space>bP ggvG"+p

eclipseaction SearchDeclarations org.eclipse.jdt.ui.edit.text.java.search.declarations.in.project
nnoremap <Space>ss :SearchDeclarations<CR>
