{
    "files.trimTrailingWhitespace": true,
    "editor.insertSpaces": true,
    "editor.fontFamily": "'Inconsolata', 'Consolas', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 14,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "workbench.colorCustomizations": {
        "editor.background": "#000000"
    },
    "workbench.colorTheme": "Dark Atom Dark Green Cursor",
    "todohighlight.keywordsPattern": "FIXME|ASAP|TODO|BUG|XXX|NOTE",
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [
                "<C-w>"
            ]
        },
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "leader",
                "H"
            ],
            "after": [
                "H"
            ]
        },
        {
            "before": [
                "leader",
                "L"
            ],
            "after": [
                "L"
            ]
        },
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "Q"
            ],
            "commands": [
                ":qa"
            ]
        },
        {
            "before": [
                "S"
            ],
            "commands": [
                ":wa"
            ]
        },
        {
            "before": [
                "<leader>",
                "p",
                "t"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "S"
            ],
            "commands": [
                "workbench.action.files.saveAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "r"
            ],
            "commands": [
                "workbench.action.quickOpenRecent"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "b"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.visualModeKeyBindings": [],
    "vim.visualModeKeyBindingsNonRecursive": []
}
