{
    "breadcrumbs.enabled": true,
    "files.trimTrailingWhitespace": true,
    "editor.autoClosingBrackets": false,
    "editor.insertSpaces": true,
    "editor.fontFamily": "'Inconsolata', 'Consolas', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 14,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80, 120],
    "editor.renderControlCharacters": true,
    "editor.minimap.enabled": false,
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "editorRuler.foreground": "#002020"
    },
    "workbench.colorTheme": "Dark Atom Dark Green Cursor",
    "workbench.editor.labelFormat": "short",
    "todohighlight.keywordsPattern": "FIXME|ASAP|TODO|BUG|XXX|NOTE",
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<backspace>"
            ],
            "after": [
                "<C-w>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-w>",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "<leader>",
                "?"
            ],
            "commands": [
                "workbench.action.openGlobalKeybindings"
            ]
        },
        {
            "before": [
                "<leader>",
                "h",
                "k"
            ],
            "commands": [
                "workbench.action.openGlobalKeybindings"
            ]
        },
        {
            "before": [
                "<leader>",
                "q",
                "q"
            ],
            "commands": [
                "workbench.action.quit"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [
                "<C-w>"
            ]
        },
        {
            "before": [
                "<leader>",
                "<left>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<leader>",
                "<right>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<leader>",
                "<up>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "<leader>",
                "<down>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "<enter>"
            ],
            "commands": [
                "extension.vim_ctrl+]"
            ]
        },
        {
            "before": [
                "\\",
                "<enter>"
            ],
            "commands": [
                "references-view.find"
            ]
        },
        {
            "before": [
                "\\",
                "<tab>"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "leader",
                "H"
            ],
            "after": [
                "H"
            ]
        },
        {
            "before": [
                "leader",
                "L"
            ],
            "after": [
                "L"
            ]
        },
        {
            "before": [
                "Z",
                "Q"
            ],
            "commands": [
                ":q!"
            ]
        },
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":x"
            ]
        },
        {
            "before": [
                "Q"
            ],
            "commands": [
                ":qa"
            ]
        },
        {
            "before": [
                "S"
            ],
            "commands": [
                ":wa"
            ]
        },
        {
            "before": [
