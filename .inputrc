set editing-mode vi

# quick debugging shortcuts for gdb (other apps do not use functional keys in readline mode anyway)
$if Gdb

# <F1>
"\eOP": "info locals\n"

# <F2>
"\eOQ": "info args\n"

# <F3>
"\eOR": "info frame\n"

# <F4>
"\eOS": "info registers\n"

# <F5>
"\e[15~": "step\n"

# <F6>
"\e[17~": "next\n"

# <F7>
"\e[18~": "finish\n"

# <F8>
"\e[19~": "continue\n"

# <F9>
"\e[20~": "info breakpoints\n"

# <F10>
"\e[21~": "delete breakpoint "

# <F11>
"\e[23~": "<F11>"

# <F12>
"\e[24~": "<F12>"

$endif # Gdb
